generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model oc_address {
  address_id  Int    @id @default(autoincrement())
  customer_id Int
  firstname   String @db.VarChar(32)
  lastname    String @db.VarChar(32)
  company     String @db.VarChar(32)
  company_id  String @db.VarChar(32)
  tax_id      String @db.VarChar(32)
  address_1   String @db.VarChar(128)
  address_2   String @db.VarChar(128)
  city        String @db.VarChar(128)
  postcode    String @db.VarChar(10)
  country_id  Int    @default(0)
  zone_id     Int    @default(0)

  @@index([customer_id], map: "customer_id")
}

model oc_address_simple_fields {
  address_id Int     @id
  metadata   String? @db.Text
}

model oc_affiliate {
  affiliate_id        Int      @id @default(autoincrement())
  firstname           String   @db.VarChar(32)
  lastname            String   @db.VarChar(32)
  email               String   @db.VarChar(96)
  telephone           String   @db.VarChar(32)
  fax                 String   @db.VarChar(32)
  password            String   @db.VarChar(40)
  salt                String   @db.VarChar(9)
  company             String   @db.VarChar(32)
  website             String   @db.VarChar(255)
  address_1           String   @db.VarChar(128)
  address_2           String   @db.VarChar(128)
  city                String   @db.VarChar(128)
  postcode            String   @db.VarChar(10)
  country_id          Int
  zone_id             Int
  code                String   @db.VarChar(64)
  commission          Decimal  @default(0.00) @db.Decimal(4, 2)
  tax                 String   @db.VarChar(64)
  payment             String   @db.VarChar(6)
  cheque              String   @db.VarChar(100)
  paypal              String   @db.VarChar(64)
  bank_name           String   @db.VarChar(64)
  bank_branch_number  String   @db.VarChar(64)
  bank_swift_code     String   @db.VarChar(64)
  bank_account_name   String   @db.VarChar(64)
  bank_account_number String   @db.VarChar(64)
  ip                  String   @db.VarChar(40)
  status              Boolean
  approved            Boolean
  date_added          DateTime @db.DateTime(0)
}

